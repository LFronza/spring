package com.lfronza.projetospring.domain;

public class Cidade {
	
	private Integer id;
	private String nome;
	
	private Estado estado;

	public Cidade() {
		// TODO Auto-generated constructor stub
	}

	public Cidade(Integer id, String nome, Estado estado) {
		super();
		this.id = id;
		this.nome = nome;
		this.estado = estado;
	}
	
	
	
}
